{
  "name": "cinemania",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://numancan.github.io/cinemania",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.13",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "format:check": "prettier -c 'src/**/*.{ts,tsx}'",
    "lint:ts": "tsc && yarn lint:eslint",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "format:ts": "prettier --write 'src/**/*.{ts,tsx}' && yarn lint:eslint --fix",
    "lint:css": "stylelint --syntax scss \"src/**/*.scss\"",
    "lint": "yarn run lint:ts && yarn run lint:css",
    "format:css": "stylelint --fix --syntax scss \"src/**/*.scss\"",
    "format": "yarn run format:ts && yarn run format:css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@atolye15/eslint-config": "^1.0.17",
    "@atolye15/stylelint-config": "^1.2.0",
    "@hot-loader/react-dom": "^16.9.0",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router-dom": "^5.1.2",
    "customize-cra": "^0.7.0",
    "eslint": "^6.4.0",
    "gh-pages": "^2.1.1",
    "node-sass": "^7.0.0",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.3",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-prettier": "^1.1.1"
  }
}
